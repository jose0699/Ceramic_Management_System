	<queryString>
		<![CDATA[SELECT e.num_expediente, 
              e.num_cedula,e.primer_nombre ||' '|| e.primer_apellido ||' '|| COALESCE(e.segundo_nombre,'') ||' '|| COALESCE(e.segundo_apellido,'') as nombre,
              to_char(h.mesano, 'TMMONTH "DE" YYYY') as fecha_turno,
              h.turno,  
		CASE WHEN h.turno = 1 THEN '7:00 am a 3:00 pm'
			 WHEN h.turno = 2 THEN '3:00 pm a 11:00 pm'
			 WHEN h.turno = 3 THEN '11:00 pm a 7:00 am'
		END AS horario
FROM empleado e, hist_turno h
WHERE h.num_expediente = e.num_expediente AND  h.mesano = date_trunc('month',$P{Mes}::timestamp) 
ORDER BY h.turno;]]>
