	<queryString>
		<![CDATA[SELECT 
	c.nombre coleccion,
	CASE
	  WHEN c.linea = 'F' THEN 'Familiar'
	  WHEN c.linea = 'I' THEN 'Institucional'
	END linea,
	
	CASE
        WHEN c.categoria = 'cou' THEN 'Country'
        WHEN c.categoria = 'cla' THEN 'Clásica'
         WHEN c.categoria = 'mod' THEN 'Moderna'
     END categoria
     
FROM coleccion c
WHERE c.uid_coleccion =  $P{id coleccion}]]>
