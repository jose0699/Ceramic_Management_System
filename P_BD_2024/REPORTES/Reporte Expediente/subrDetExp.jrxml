	<style name="isBad" mode="Opaque" forecolor="#FFFFFF" backcolor="#7DA44E">
		<conditionalStyle>
			<conditionExpression><![CDATA[$F{motivo}.equalsIgnoreCase( "AMONESTACIONES" ) || $F{motivo}.equalsIgnoreCase( "INASISTENCIAS" ) || $F{motivo}.equalsIgnoreCase( "LLEGADAS TARDE" )]]></conditionExpression>
			<style mode="Opaque" forecolor="#FFFFFF" backcolor="#C11818"/>
		</conditionalStyle>
